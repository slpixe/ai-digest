{
  "name": "ai-txt",
  "version": "1.4.3",
  "description": "CLI tool to aggregate files into a single Markdown file",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "ai-md": "dist/cli.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "local-run": "npx ai-md --whitespace-removal --show-output-files",
    "publish": "npm publish --access public",
    "version": "node scripts/sync-version.js && git add jsr.json",
    "preversion": "npm test",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "keywords": [
    "cli",
    "aggregate",
    "markdown"
  ],
  "author": "@slpixe",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "gpt-tokenizer": "^2.8.1",
    "ignore": "^7.0.3",
    "isbinaryfile": "^5.0.4",
    "p-limit": "^6.2.0",
    "winston": "^3.17.0"
  },
  "homepage": "https://github.com/slpixe/ai-md#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slpixe/ai-md.git"
  },
  "bugs": {
    "url": "https://github.com/slpixe/ai-md/issues"
  },
  "jsr": {
    "name": "@slpixe/ai-md",
    "namespace": "slpixe"
  }
}
