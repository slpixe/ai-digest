{
  "name": "ai-txt",
  "version": "1.2.1",
  "description": "CLI tool to aggregate files into a single Markdown file",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ai-md": "dist/index.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "local-run": "npx ai-md --whitespace-removal --show-output-files",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "cli",
    "aggregate",
    "markdown"
  ],
  "author": "@slpixe",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "ignore": "^7.0.3",
    "isbinaryfile": "^5.0.4",
    "js-tiktoken": "^1.0.18",
    "p-limit": "^6.2.0",
    "winston": "^3.17.0"
  },
  "homepage": "https://github.com/slpixe/ai-md#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slpixe/ai-md.git"
  },
  "bugs": {
    "url": "https://github.com/slpixe/ai-md/issues"
  },
  "jsr": {
    "name": "@slpixe/ai-md",
    "namespace": "slpixe"
  }
}
